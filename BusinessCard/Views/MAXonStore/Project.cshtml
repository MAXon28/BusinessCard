@{
    ViewData["Title"] = "Проект";
}
<!DOCTYPE html>
<html lang="ru">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/css/project.css" runat="server" />
    <link rel="stylesheet" href="~/css/preloader.css" runat="server" />
    <link rel="stylesheet" href="~/css/font-awesome.css" runat="server" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css" runat="server" />
    <link rel="stylesheet" href="~/css/chief-slider.min.css">
    <script defer src="~/js/chief-slider.min.js"></script>
    <script src="https://use.fontawesome.com/5d4de9dc4f.js"></script>
    <title>@ViewData["Title"] | MAXon28</title>
</head>
<body class="body-project">
    <div class="scene-project">
        <div id="dataSpinner" class="reverse-spinner delete">
            <div class="status">Загрузка<span class="status__dot">.</span><span class="status__dot">.</span><span class="status__dot">.</span></div>
        </div>
        <div id="data" class="data delete">
            <div class="mainBlock"></div>
            <div class="informationBlock">
                <div id="screenshots" class="dataBlock skip">
                    <p class="header">Скриншоты</p>
                    <div class="separator horizontal"></div>
                </div>
                <div id="video" class="dataBlock skip">
                    <p class="header">Видео</p>
                    <div class="separator horizontal"></div>
                </div>
                <div id="code" class="dataBlock skip">
                    <p class="header">Разработка</p>
                    <div class="separator horizontal"></div>
                </div>
                <div id="reviews" class="dataBlock skip">
                    <p class="header">Отзывы</p>
                    <div class="separator horizontal"></div>
                    <div class="reviewStatisticBlock">
                        <div class="generalStatisticBlock">
                            <p class="informationText" style="font-size: 67px; width: 93.14px; text-align: center; margin: 5px 28px; line-height: normal;"></p>
                            <p class="additionalText"></p>
                        </div>
                        <div class="detailStatisticBlock">
                            <div id="fiveStars" class="ratingStatisticBlock">
                                <p class="informationText" style="font-size: 12px; margin: 5px; line-height: normal;">5</p>
                                <label class="rating__item" style="color: gold; font-size: 12px;">
                                    <svg class="rating__star"><use xlink:href="#star"></use></svg>
                                </label>
                                <div class="animated-progress progress">
                                    <span></span>
                                </div>
                            </div>
                            <div id="fourStars" class="ratingStatisticBlock">
                                <p class="informationText" style="font-size: 12px; margin: 5px; line-height: normal;">4</p>
                                <label class="rating__item" style="color: gold; font-size: 12px;">
                                    <svg class="rating__star"><use xlink:href="#star"></use></svg>
                                </label>
                                <div class="animated-progress progress">
                                    <span></span>
                                </div>
                            </div>
                            <div id="threeStars" class="ratingStatisticBlock">
                                <p class="informationText" style="font-size: 12px; margin: 5px; line-height: normal;">3</p>
                                <label class="rating__item" style="color: gold; font-size: 12px;">
                                    <svg class="rating__star"><use xlink:href="#star"></use></svg>
                                </label>
                                <div class="animated-progress progress">
                                    <span></span>
                                </div>
                            </div>
                            <div id="twoStars" class="ratingStatisticBlock">
                                <p class="informationText" style="font-size: 12px; margin: 5px; line-height: normal;">2</p>
                                <label class="rating__item" style="color: gold; font-size: 12px;">
                                    <svg class="rating__star"><use xlink:href="#star"></use></svg>
                                </label>
                                <div class="animated-progress progress">
                                    <span></span>
                                </div>
                            </div>
                            <divi d="oneStars" class="ratingStatisticBlock">
                                <p class="informationText" style="font-size: 12px; margin: 5px; line-height: normal;">1</p>
                                <label class="rating__item" style="color: gold; font-size: 12px;">
                                    <svg class="rating__star"><use xlink:href="#star"></use></svg>
                                </label>
                                <div class="animated-progress progress">
                                    <span></span>
                                </div>
                            </divi>
                        </div>
                    </div>
                    <div id="reviewSpinner" class="reverse-spinner delete">
                        <div class="status">Загрузка<span class="status__dot">.</span><span class="status__dot">.</span><span class="status__dot">.</span></div>
                    </div>
                    <button class="createReview" onclick="CreateReview()">Оставить отзыв</button>
                </div>
                <div id="information" class="dataBlock skip">
                    <p class="header">О приложении</p>
                    <div class="separator horizontal"></div>
                </div>
                <div id="technicalRequirement" class="dataBlock skip">
                    <p class="header">Технические требования</p>
                    <div class="separator horizontal"></div>
                </div>
            </div>
        </div>
    </div>
    <svg xmlns="http://www.w3.org/2000/svg" style="width: 0; height: 0;">
        <defs>
            <symbol id="star" viewBox="0 0 28 28">
                <path d="M26 10.109c0 .281-.203.547-.406.75l-5.672 5.531 1.344 7.812c.016.109.016.203.016.313 0 .406-.187.781-.641.781a1.27 1.27 0 0 1-.625-.187L13 21.422l-7.016 3.687c-.203.109-.406.187-.625.187-.453 0-.656-.375-.656-.781 0-.109.016-.203.031-.313l1.344-7.812L.39 10.859c-.187-.203-.391-.469-.391-.75 0-.469.484-.656.875-.719l7.844-1.141 3.516-7.109c.141-.297.406-.641.766-.641s.625.344.766.641l3.516 7.109 7.844 1.141c.375.063.875.25.875.719z"/>
            </symbol>
        </defs>
    </svg>
    <svg xmlns="http://www.w3.org/2000/svg" width="28px" height="28px" viewBox="0 0 28 28" style="display: none">
        <g id="preloaderCreationReview">
            <path fill="currentColor"
                d="M304 48c0 26.51-21.49 48-48 48s-48-21.49-48-48 21.49-48 48-48 48 21.49 48 48zm-48 368c-26.51 0-48 21.49-48 48s21.49 48 48 48 48-21.49 48-48-21.49-48-48-48zm208-208c-26.51 0-48 21.49-48 48s21.49 48 48 48 48-21.49 48-48-21.49-48-48-48zM96 256c0-26.51-21.49-48-48-48S0 229.49 0 256s21.49 48 48 48 48-21.49 48-48zm12.922 99.078c-26.51 0-48 21.49-48 48s21.49 48 48 48 48-21.49 48-48c0-26.509-21.491-48-48-48zm294.156 0c-26.51 0-48 21.49-48 48s21.49 48 48 48 48-21.49 48-48c0-26.509-21.49-48-48-48zM108.922 60.922c-26.51 0-48 21.49-48 48s21.49 48 48 48 48-21.49 48-48-21.491-48-48-48z">
            </path>
        </g>
    </svg>
    <script src="~/js/jquery-3.5.1.js"></script> 
    <script src="~/js/project.js"></script>
</body>
</html>